
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    @foreach (KeyValuePair<string, int> productPrices in ViewBag.ProductPrices)
    {
        <div>
            <div>
                Product name: @productPrices.Key
            </div>
            <div>
                Product price including tax: @(productPrices.Value * 1.2)
            </div>
        </div>
    }
</body>
</html>
